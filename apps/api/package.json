    "@cost-watchdog/connectors": "workspace:*",
    "@cost-watchdog/core": "workspace:*",
    "@fastify/cors": "^9.0.0",
    "@fastify/jwt": "^8.0.0",
    "@fastify/multipart": "^8.1.0",
    "@fastify/rate-limit": "^9.1.0",
    "@node-rs/argon2": "^1.7.0",
    "@prisma/client": "^5.10.0",
    "@sinclair/typebox": "^0.34.41",
    "bullmq": "^5.1.0",
    "fastify": "^4.26.0",
    "fastify-plugin": "^4.5.0",
